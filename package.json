{
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "generate": "yarn generate:gql && yarn generate:types",
    "generate:gql": "graphql-codegen -r dotenv/config",
    "generate:types": "typechain --target ethers-v5 --out-dir generated/types src/artifacts/*.json",
    "build": "yarn generate && next build",
    "format": "prettier --write .",
    "lint": "next lint",
    "start": "next start",
    "postinstall": "yarn generate:types && yarn generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@rainbow-me/rainbowkit": "^0.2.2",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "@web3-react/walletlink-connector": "^6.2.11",
    "clsx": "^1.1.1",
    "ethers": "^5.5.4",
    "graphql": "^16.2.0",
    "graphql-tag": "^2.12.6",
    "next": "^12.0.8",
    "next-themes": "^0.1.1",
    "react": "^18.0.0",
    "react-aria": "^3.12.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.2.0",
    "react-query": "^3.34.15",
    "wagmi": "^0.4.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.5.0",
    "husky": ">=6",
    "lint-staged": ">=12",
    "postcss": "^8.4.5",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.13",
    "typechain": "^8.0.0",
    "typescript": "^4.7.2"
  }
}
